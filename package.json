{
  "name": "redmine-tracker",
  "productName": "RedmineTracker",
  "version": "1.1.1",
  "description": "Redmine time tracker.",
  "author": {
    "name": "Jan Strnadek",
    "email": "jan.strnadek@eman.cz",
    "url": "https://github.com/Strnadj"
  },
  "main": "main.js",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors --display-error-details",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "release": "build",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "package-linux": "npm run build && build --linux",
    "package-win": "npm run build && build --win --x64",
    "postinstall": "yarn run flow-typed && yarn run electron-rebuild && yarn run build-dll && yarn run electron-builder install-app-deps && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js --progress",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js",
    "test-all": "npm run lint && npm run flow && npm run build && npm run test && npm run test-e2e",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js e2e",
    "test-watch": "npm test -- --watch",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "browserslist": "electron 2.0",
  "build": {
    "productName": "RedmineTracker",
    "appId": "cz.eman.redmine_tracker",
    "publish": [
      {
        "provider": "github",
        "owner": "eManPrague",
        "repo": "redmine_tracker"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 200,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "entries.html",
      "edit.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "app/assets/",
        "to": "assets/"
      }
    ],
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Office"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eManPrague/redmine_tracker.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eManPrague/redmine_tracker/issues"
  },
  "keywords": [
    "electron",
    "redmine",
    "issue",
    "tracker"
  ],
  "homepage": "https://github.com/eManPrague/redmine_tracker",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "boiler-room-custodian": "^0.6.1",
    "concurrently": "^3.1.0",
    "cross-env": "^5.0.2",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "electron": "2.0.4",
    "electron-builder": "20.16.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.7.3",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.3",
    "fbjs-scripts": "^0.8.3",
    "file-loader": "1.1.11",
    "flow-bin": "^0.75.0",
    "flow-runtime": "^0.14.0",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "jsdom": "^9.9.1",
    "minimist": "^1.2.0",
    "node-abi": "^2.4.0",
    "node-loader": "^0.6.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^2.7.4",
    "sinon": "^1.17.7",
    "spectron": "^3.4.1",
    "style-loader": "^0.15.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "uglifyjs-webpack-plugin": "1.0.0-beta.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-debug": "2.0.0",
    "electron-json-storage": "3.1.1",
    "electron-log": "^2.2.15",
    "electron-redux": "^1.3.0",
    "electron-updater": "^2.16.1",
    "extract-text-webpack-plugin": "3.0.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "keytar": "^4.2.1",
    "moment": "^2.19.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.1",
    "react-hot-loader": "3.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-select": "^1.0.0",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "request": "^2.84.0",
    "shortid": "^2.2.8",
    "source-map-support": "^0.4.18",
    "valid-url": "^1.0.9",
    "snyk": "^1.316.1"
  },
  "devEngines": {
    "node": ">= 8.3.0",
    "npm": ">= 5.4.2"
  },
  "snyk": true
}
