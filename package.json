{
  "name": "redmine-tracker",
  "productName": "RedmineTracker",
  "version": "1.2.0-rc1",
  "description": "Redmine time tracker.",
  "author": {
    "name": "Jan Strnadek",
    "email": "jan.strnadek@eman.cz",
    "url": "https://github.com/Strnadj"
  },
  "main": "main.js",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors --display-error-details",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "release": "build",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "package-linux": "npm run build && build --linux",
    "package-win": "npm run build && build --win --x64",
    "postinstall": "yarn run electron-rebuild && yarn run build-dll && yarn run electron-builder install-app-deps && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js --progress",
    "test": "cross-env NODE_ENV=test node --trace-warnings ./test/runTests.js",
    "test-all": "npm run lint && npm run build && npm run test && npm run test-e2e",
    "test-e2e": "cross-env NODE_ENV=test node --trace-warnings ./test/runTests.js e2e",
    "test-watch": "npm test -- --watch"
  },
  "browserslist": "electron 2.0",
  "build": {
    "productName": "RedmineTracker",
    "appId": "cz.eman.redmine_tracker",
    "publish": [
      {
        "provider": "github",
        "owner": "eManPrague",
        "repo": "redmine_tracker"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 200,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "entries.html",
      "edit.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "app/assets/",
        "to": "assets/"
      }
    ],
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Office"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eManPrague/redmine_tracker.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eManPrague/redmine_tracker/issues"
  },
  "keywords": [
    "electron",
    "redmine",
    "issue",
    "tracker"
  ],
  "homepage": "https://github.com/eManPrague/redmine_tracker",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ]
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "boiler-room-custodian": "^0.6.1",
    "concurrently": "^3.1.0",
    "cross-env": "^5.0.2",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "electron": "4.0.1",
    "electron-builder": "20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "express": "^4.15.3",
    "fbjs-scripts": "^0.8.3",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "jsdom": "^9.9.1",
    "minimist": "^1.2.0",
    "node-abi": "^2.5.1",
    "node-loader": "^0.6.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^2.7.4",
    "sinon": "^1.17.7",
    "spectron": "^3.4.1",
    "style-loader": "^0.15.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "4.28.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "connected-react-router": "6.1.0",
    "electron-debug": "2.0.0",
    "electron-json-storage": "4.1.5",
    "electron-log": "^2.2.17",
    "electron-redux": "1.3.1",
    "electron-updater": "4.0.6",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "keytar": "^4.2.1",
    "moment": "^2.19.3",
    "react": "^16.7.0",
    "react-dom": "^16.0.1",
    "react-hot-loader": "3.0.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.0.0",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.84.0",
    "shortid": "^2.2.8",
    "source-map-support": "^0.4.18",
    "valid-url": "^1.0.9"
  },
  "devEngines": {
    "node": ">= 8.3.0",
    "npm": ">= 5.4.2"
  }
}
